{
  "name": "typeshift",
  "version": "0.0.1",
  "description": "TypeShift is a cli tool that enables \"code first\" schema generation from TypeScript",
  "main": "index.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "scripts": {
    "build": "yarn build:app",
    "prebuild": "rimraf 'lib/*'",
    "postbuild": "yarn build:addon && yarn copy:addon",
    "build:addon": "node scripts/build-addon",
    "build:app": "tsc -b",
    "copy:addon": "copyfiles --error --up 2 src/generator/*.node lib/generator",
    "test": "mocha",
    "pretest": "yarn build:addon",
    "start": "node -r ts-node/register ./src/typeshift.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/engagedtheory/typeshift.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/engagedtheory/typeshift/issues"
  },
  "homepage": "https://github.com/engagedtheory/typeshift#readme",
  "dependencies": {
    "commander": "^10.0.0",
    "fast-glob": "^3.2.12",
    "openapi-types": "^12.1.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "cargo-cp-artifact": "^0.1.7",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1"
  }
}
