{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Native",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/native-debug/target/debug/native-debug",
            "args": [],
            "cwd": "${workspaceFolder}/native-debug",
            "terminal": "integrated",
            "stopOnEntry": false,
            "envFile": "${workspaceFolder}/native-debug/.envrc",
            "preLaunchTask": "rust: build native-debug",
            "sourceLanguages": [
                "rust"
            ]
        },
        {
            "name": "Debug Native v3 Tests",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--manifest-path",
                    "${workspaceFolder}/native_v3/Cargo.toml",
                    "--lib",
                    "--",
                    "--nocapture",
                ],
                "env": {
                    "RUST_BACKTRACE": "1",
                    "RUST_LOG": "debug",
                    "RUST_DEBUG": "true",
                },
                "filter": {
                    "name": "lil_schemy",
                    "kind": "lib"
                },
            },
            "args": [],
            "terminal": "integrated",
            "stopOnEntry": false,
            "envFile": "${workspaceFolder}/native_v3/.envrc",
            "sourceLanguages": [
                "rust"
            ]
        },
        {
            "name": "Debug Node",
            "type": "node",
            "request": "launch",
            "preLaunchTask": "npm: pretest",
            "program": "node",
            "args": [
                "node_modules/mocha/bin/mocha.js",
                "--grep",
                "generates schemas"
            ],
            "cwd": "${workspaceFolder}"
        },
    ]
}